DEBUG=true
CORS_ORIGINS=*

# OpenSearch
OS_HOST=opensearch
OS_PORT=9200
OS_USER=admin
OS_PASSWORD=admin
OS_USE_SSL=false
OS_VERIFY_CERTS=false
OS_INDEX_TRACKS=moodtune_tracks

# OpenAI
OPENAI_API_KEY=sk-...
OPENAI_MODEL=gpt-4o-mini
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
OPENAI_EMBEDDING_DIM=1536

MIN_TRACKS=20

# External services
MUSIC_SERVICE_URL=http://localhost:8020

# Music service provider (external)
# The music service internally can resolve via iTunes or Spotify
# See moodtune_music/.env.example

# Ingestion limits (per emotion)
INGEST_LIMIT_PER_EMOTION=200

# Dev convenience
AUTO_SEED_ON_START=true
